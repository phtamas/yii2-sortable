{
    "name": "phtamas/yii2-sortable",
    "description": "Sortable behavior for Yii2 Active Record",
    "keywords": [
        "yii2",
        "behavior",
        "sortable"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Papp-Horváth Tamás",
            "email": "phtamas@gmail.com"
        }
    ],
    "minimum-stability": "stable",

    "require": {
        "yiisoft/yii2": "~2.0.7"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.8.5",
        "phpunit/dbunit": "~1.4.1"
    },

    "autoload": {
        "psr-4": {
            "phtamas\\yii2\\sortable\\": "src/"
        }
    },

    "autoload-dev": {
        "phtamas\\yii2\\sortable\\tests\\": "tests/"
    }
